{
	"name": "transcendence-backend",
	"version": "1.0.0",
	"scripts": {
		"dev": "concurrently \"vite\" \"npm run dev:backend\"",
		"dev:backend": "ts-node-dev --ignore-watch node_modules --respawn --transpile-only src/backend/server.ts",
		"dev:frontend": "vite",
		"build": "npm run db:generate && npm run build:frontend && webpack --config webpack.config.ts",
		"build:frontend": "vite build",
		"start": "npm run build && node dist/server.js",
		"db:push": "prisma db push",
		"db:reset": "prisma migrate reset --force",
		"db:generate": "prisma generate",
		"db:studio": "prisma studio",
		"postinstall": "npm run db:generate"
	},
	"keywords": [],
	"author": "sandoramix",
	"license": "MIT",
	"devDependencies": {
		"@types/passport-google-oauth20": "^2.0.16",
		"@types/webpack-node-externals": "^3.0.4",
		"concurrently": "^9.1.2",
		"pino-pretty": "^13.0.0",
		"prisma": "^6.7.0",
		"ts-loader": "^9.5.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.8.3",
		"vite-node": "^3.1.4",
		"webpack": "^5.99.8",
		"webpack-cli": "^6.0.1",
		"webpack-node-externals": "^3.0.0"
	},
	"dependencies": {
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.0.1",
		"@fastify/flash": "^6.0.3",
		"@fastify/formbody": "^8.0.2",
		"@fastify/passport": "^3.0.2",
		"@fastify/session": "^11.1.0",
		"@fastify/static": "^8.2.0",
		"@prisma/client": "^6.7.0",
		"@trpc/client": "^11.1.4",
		"@trpc/server": "^11.1.4",
		"dotenv": "^16.5.0",
		"fastify": "^5.3.2",
		"fastify-type-provider-zod": "^4.0.2",
		"npm": "^11.3.0",
		"passport-google-oauth20": "^2.0.0",
		"superjson": "^2.2.2",
		"vite": "^6.3.5",
		"zod": "^3.24.4"
	}
}
