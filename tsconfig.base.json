{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "paths": {
      "@backend/*": [
        "./backend/*"
      ],
      "@frontend/*": [
        "./frontend/*"
      ]
    }
  },
  "include": [
	  "./backend/env.ts"
  ]
}
